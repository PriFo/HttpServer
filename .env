### HTTP server
SERVER_PORT=9999

### Database paths
DATABASE_PATH=data.db
NORMALIZED_DATABASE_PATH=normalized_data.db
SERVICE_DATABASE_PATH=service.db

### AI configuration
ARLIAI_API_KEY=changeme
ARLIAI_MODEL=GLM-4.5-Air

### Database pooling
DB_MAX_OPEN_CONNS=25
DB_MAX_IDLE_CONNS=5
DB_CONN_MAX_LIFETIME=5m

### Logging & buffering
LOG_BUFFER_SIZE=100
NORMALIZER_EVENTS_BUFFER_SIZE=100

### Multi-provider normalization
MULTI_PROVIDER_ENABLED=false
AGGREGATION_STRATEGY=first_success
AI_TIMEOUT=30s

### Enrichment
ENRICHMENT_ENABLED=true
ENRICHMENT_AUTO_ENRICH=true
ENRICHMENT_MIN_QUALITY_SCORE=0.3
ENRICHMENT_CACHE_TTL=24h
ENRICHMENT_CACHE_CLEANUP=1h

# Dadata
DADATA_ENABLED=true
DADATA_API_KEY=
DADATA_SECRET_KEY=
DADATA_BASE_URL=https://suggestions.dadata.ru
DADATA_TIMEOUT=30s
DADATA_MAX_REQUESTS=100
DADATA_PRIORITY=1

# Adata
ADATA_ENABLED=true
ADATA_API_KEY=
ADATA_BASE_URL=https://adata.kz
ADATA_TIMEOUT=30s
ADATA_MAX_REQUESTS=50
ADATA_PRIORITY=2

# GISP
GISP_ENABLED=false
GISP_API_KEY=
GISP_BASE_URL=https://gisp.gov.ru
GISP_TIMEOUT=30s
GISP_MAX_REQUESTS=50
GISP_PRIORITY=3

### Web search validation
WEB_SEARCH_ENABLED=true
WEB_SEARCH_TIMEOUT=5s
WEB_SEARCH_CACHE_TTL=24h
