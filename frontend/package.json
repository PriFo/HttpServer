{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:all",
    "dev:frontend": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "analyze": "ANALYZE=true npm run build",
    "dev:backend": "cd .. && go run main.go",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:with-backend": "npm run dev:all",
    "todos:scan": "cd .. && bash .todos/scripts/scan-todos.sh .",
    "todos:report": "cd .. && node .todos/scripts/generate-report.js",
    "todos:assign": "cd .. && python3 .todos/scripts/assign-tasks.py",
    "todos:workload": "cd .. && python3 .todos/scripts/update-workload.py",
    "todos:validate": "cd .. && bash .todos/scripts/validate-tasks.sh",
    "todos:stats": "cd .. && bash .todos/scripts/stats.sh",
    "todos:export": "cd .. && node .todos/scripts/export-tasks.js",
    "todos:cleanup": "cd .. && bash .todos/scripts/cleanup-resolved.sh 30",
    "todos:all": "npm run todos:scan && npm run todos:assign && npm run todos:workload && npm run todos:report",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:full": "playwright test tests/e2e/full-project-e2e.spec.ts",
    "test:e2e:roles": "playwright test tests/e2e/user-roles.spec.ts",
    "test:e2e:quality": "playwright test tests/e2e/quality-management.spec.ts",
    "test:e2e:data": "playwright test tests/e2e/data-management.spec.ts",
    "test:e2e:integration": "playwright test tests/e2e/integration.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run",
    "test:integration": "playwright test tests/integration/quality-api.test.ts",
    "test:quality": "playwright test tests/e2e/quality-error-handling.spec.ts"
  },
  "dependencies": {
