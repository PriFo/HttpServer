    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
